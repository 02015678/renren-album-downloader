{
  "name": "Renren Album Downloader",
  "version": "0.6",
  "manifest_version": 2,
  "description": "",
  "permissions": [
    "http://*.rrimg.com/",
    "http://*.rrfmn.com/",
    "http://*.xnpic.com/",
    "http://*.xnimg.cn/",
    "http://*.xiaonei.com/",
    "http://photo.renren.com/photo/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://photo.renren.com/photo/*album-*"],
      "css": ["style.css"],
      "js": ["lib/jquery-binary-ajax.js", "lib/jszip.js", "lib/base64ArrayBuffer.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    "ui/arrow.png"
  ]
}
