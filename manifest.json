{
  "name": "__MSG_extName__",
  "version": "0.7.10",
  "manifest_version": 2,
  "description": "__MSG_extDesc__",
  "permissions": [
    "http://*.rrimg.com/",
    "http://*.rrfmn.com/",
    "http://*.xnpic.com/",
    "http://*.xnimg.cn/",
    "http://*.xiaonei.com/",
    "http://photo.renren.com/photo/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://photo.renren.com/getalbumprofile.do?owner=*", "http://photo.renren.com/photo/*album-*"],
      "js": ["lib/jquery-binary-ajax.js", "lib/jszip.js", "lib/base64ArrayBuffer.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "style.css",
    "ui/download.png",
    "ui/checkmark.png"
  ],
  "default_locale": "zh_CN",
  "icons": {
    "16": "ui/logo_16.png",
    "48": "ui/logo_48.png",
    "128": "ui/logo_128.png"
  }
}
