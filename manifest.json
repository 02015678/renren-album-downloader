{
  "name": "Renren Album Downloader",
  "version": "0.5",
  "manifest_version": 2,
  "description": "",
  "permissions": [
    "http://*.rrimg.com/",
    "http://*.rrfmn.com/",
    "http://*.xnpic.com/",
    "http://*.xnimg.cn/",
    "http://*.xiaonei.com/",
    "http://photo.renren.com/photo/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["http://photo.renren.com/photo/*album-*"],
      "css": ["style.css"],
      "js": ["jquery-binary-ajax.js", "jszip.js", "base64ArrayBuffer.js", "content.js"]
    }
  ]
}
